    "keycharm": "^0.3.1",
    "modernizr": "^3.11.2",
    "moment": "^2.24.0",
    "timsort": "^0.3.0",
    "uuid": "^7.0.3",
    "vis-data": "^6.5.1",
    "vis-network": "^7.5.2",
    "vis-util": "^4.0.0",
    "y-websocket": "^1.2.5",
    "yjs": "^13.0.5"
  },
  "devDependencies": {
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "2.0.5",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "private": true
