{
	"name": "participatory-system-mapper",
	"version": "1.2.0",
	"description": "Participatory System Mapper, browser app for collaboratively creating system maps",
	"keywords": [
		"social network",
		"participatory",
		"stakeholder",
		"policy analysis"
	],
	"bugs": {
		"email": "n.gilbert@surrey.ac.uk"
	},
	"author": "Nigel Gilbert",
	"browser": "prsm.html",
	"repository": {
		"type": "git",
		"url": "https://github.com/micrology/prsm"
	},
	"scripts": {
		"lint": "./node_modules/.bin/eslint ./js/*.js --fix --quiet",
		"pretty": "prettier --write ./html/*.html ./js/*.js ./css/prsm.css doc/**/*.{html,css} package.json",
		"test": "echo \"Error: no test specified\" && exit 1",
		"build-help": "pandoc README.md --metadata title='PRSM Help' -f gfm -t html5 -c doc/help.css -H doc/meta.html -s --toc -o ./help.html",
		"check": "npm run --silent pretty && npm run --silent lint && npm run --silent build-help",
		"dev": "parcel ./html/prsm.html",
		"build": "rm -rf dist/* && parcel build ./html/prsm.html --public-url . ",
		"doc": "npx jsdoc -r ./js -R README.md -P ./package.json",
		"clean": "rm -rf dist/* out/*"
	},
	"license": "GPL-3.0-or-later",
	"dependencies": {
		"@egjs/hammerjs": "^2.0.17",
		"@joeattardi/emoji-button": "^2.12.1",
		"component-emitter": "^1.3.0",
		"fast-xml-parser": "^3.17.4",
		"hammerjs": "^2.0.8",
		"keycharm": "^0.3.1",
		"moment": "^2.27.0",
		"timsort": "^0.3.0",
		"uuid": "^7.0.3",
		"vis-data": "^6.6.1",
		"vis-network": "^7.10.2",
		"vis-util": "^4.3.3",
		"y-indexeddb": "^9.0.5",
		"y-websocket": "^1.3.1",
		"yjs": "^13.3.0"
	},
	"devDependencies": {
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"jsdoc": "^3.6.5",
		"parcel-bundler": "^1.12.4",
		"prettier": "2.0.5"
	},
	"private": true
}
