  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dist": "webpack --mode=development",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open-page prism.html",
    "postinstall": "npm run dist",
    "lint": "./node_modules/.bin/eslint ./js/*.js --fix --quiet",
    "pretty": "prettier --write prism.html ./js/*.js ./css/prism.css",
    "test": "echo \"Error: no test specified\" && exit 1"
